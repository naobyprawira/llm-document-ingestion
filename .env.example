# API Keys
GOOGLE_API_KEY=your_google_api_key_here
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_URL=https://your-qdrant-instance.qdrant.io:6333

# Database and Model Configuration
QDRANT_COLLECTION=your-collection_name
GEMINI_MODEL=gemini-2.5-flash
BM25_MODEL=Qdrant/bm25
DENSE_EMBEDDING_MODEL=gemini-embedding-001

# VLM Settings
IMG_MAX_SIDE=1600
VLM_MAX_TOKENS=1512
VLM_TEMPERATURE=0.1
IMG_JPEG_QUALITY=85
RETRY_MAX=3
RETRY_BASE_SEC=0.8

# Other Settings
BM25_CACHE_DIR=.cache_fastembed
EMBED_BATCH_SIZE=32
MAX_WORKERS=5